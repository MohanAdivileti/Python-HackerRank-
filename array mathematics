import numpy as np

if __name__ == "__main__":
    n, m = map(int, input().split())

    ar1 = np.array([input().split() for _ in range(n)], int)
    ar2 = np.array([input().split() for _ in range(n)], int)

    print(ar1 + ar2)
    print(ar1 - ar2)
    print(ar1 * ar2)
    print(ar1 // ar2)
    print(ar1 % ar2)
    print(ar1**ar2)
